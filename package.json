{
  "name": "@ematu/dpd-sdk",
  "version": "1.0.0",
  "description": "Enterprise-grade TypeScript SDK for DPD Poland API with full type safety and runtime validation",
  "author": "Ematu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ematu/dpd-sdk.git"
  },
  "homepage": "https://github.com/ematu/dpd-sdk#readme",
  "bugs": {
    "url": "https://github.com/ematu/dpd-sdk/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint:check && npm run format:check",
    "prepublishOnly": "npm run validate && npm run build"
  },
  "keywords": [
    "dpd",
    "dpd-poland",
    "shipping",
    "logistics",
    "courier",
    "api-client",
    "sdk",
    "typescript",
    "soap",
    "tracking",
    "parcel",
    "delivery",
    "pudo",
    "cod",
    "insurance",
    "enterprise"
  ],
  "dependencies": {
    "date-fns": "^3.3.1",
    "soap": "^1.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "prettier": "^3.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
